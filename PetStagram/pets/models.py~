from django.db import models


class Pet(models.Model):
    name = models.CharField(
        max_length=30,
    )

    perosnal_photo = models.URLField()

    date_of_birth = models.DateField(
        null=True,
        blank=True,
    )

    slug = models.SlugField(
        unique=True,
        null=False,
        blank=True,
    )

    def save(self, *args, **kwargs):

    def __str__(self):
        return self.name
